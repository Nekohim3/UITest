<Page x:Class="UITest.Views.Questionnaires"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UITest.Views"
      xmlns:viewModels="clr-namespace:UITest.ViewModels"
      mc:Ignorable="d" d:DataContext="{d:DesignInstance viewModels:QuestionnairesViewModel}"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Questionnaires">
    <Grid>
        <StackPanel Width="150">
            <TextBlock Text="Questionnaires" TextAlignment="Center"/>
            <Button Content="Посмотреть вопросник" Margin="2" Command="{Binding ViewCmd}"/>
            <Button Content="Редактировать вопросник" Margin="2" Command="{Binding EditCmd}"/>
            <Button Content="Ответник" Margin="2" Command="{Binding AnsCmd}"/>
            <Button Content="История" Margin="2" Command="{Binding HistoryCmd}"/>
        </StackPanel>
    </Grid>
</Page>
